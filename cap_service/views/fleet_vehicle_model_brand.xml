<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="fleet_vehicle_model_brand_view_form_service" model="ir.ui.view">
        <field name="name">fleet_vehicle_model_brand_view_form_service</field>
        <field name="model">fleet.vehicle.model.brand</field>
        <field name="inherit_id" ref="fleet.fleet_vehicle_model_brand_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="after">
                <field name="is_bus_fleet"/>
            </xpath>
        </field>
    </record>
</odoo>
